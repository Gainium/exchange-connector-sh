{
  "name": "exchange-connector",
  "version": "1.0.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "(rm -rf ./dist || true) && node esbuild.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:prod": "node dist/main.js",
    "lint": "tsc --noEmit --project tsconfig.json && eslint . --cache",
    "lint:fix": "eslint . --fix",
    "binance:test": "ts-node -r dotenv/config --files --project tsconfig.json ./src/exchange/exchanges/binance/test.ts",
    "bybit:test": "ts-node -r dotenv/config --files --project tsconfig.json ./src/exchange/exchanges/bybit/test.ts",
    "bitget:test": "ts-node -r dotenv/config --files --project tsconfig.json ./src/exchange/exchanges/bitget/test.ts",
    "kucoin:test": "ts-node -r dotenv/config --files --project tsconfig.json ./src/exchange/exchanges/kucoin/test.ts",
    "okx:test": "ts-node -r dotenv/config --files --project tsconfig.json ./src/exchange/exchanges/okx/test.ts",
    "coinbase:test": "ts-node -r dotenv/config --files --project tsconfig.json ./src/exchange/exchanges/coinbase/test.ts",
    "push": "git push origin main",
    "pull": "git pull origin main",
    "fullInit": "export GAINIUM_KUCOIN=@gainium/kucoin-api && (yarn remove $GAINIUM_KUCOIN || true) && yarn cache clean $GAINIUM_KUCOIN && yarn add $GAINIUM_KUCOIN@git+https://github.com/Gainium/kucoin-api.git && unset GAINIUM_KUCOIN",
    "prepare": "husky"
  },
  "dependencies": {
    "@gainium/kucoin-api": "git+https://github.com/Gainium/kucoin-api.git",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "axios": "^1.6.7",
    "binance-api-node": "^0.12.0",
    "bitget-api": "^2.0.13",
    "bybit-api": "^3.3.3",
    "coinbase-advanced-node": "^3.0.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "isomorphic-unfetch": "^3.1.0",
    "okx-api": "^1.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "ts-node": "^10.0.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
