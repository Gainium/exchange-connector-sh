{
  "name": "exchange-connector",
  "version": "1.0.4",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "(rm -rf ./dist || true) && node esbuild.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:prod": "node dist/main.js",
    "lint": "tsc --noEmit --project tsconfig.json && eslint . --cache",
    "lint:fix": "eslint . --fix",
    "binance:test": "ts-node -r dotenv/config --files --project tsconfig.json ./src/exchange/exchanges/binance/test.ts",
    "bybit:test": "ts-node -r dotenv/config --files --project tsconfig.json ./src/exchange/exchanges/bybit/test.ts",
    "bitget:test": "ts-node -r dotenv/config --files --project tsconfig.json ./src/exchange/exchanges/bitget/test.ts",
    "kucoin:test": "ts-node -r dotenv/config --files --project tsconfig.json ./src/exchange/exchanges/kucoin/test.ts",
    "okx:test": "ts-node -r dotenv/config --files --project tsconfig.json ./src/exchange/exchanges/okx/test.ts",
    "coinbase:test": "ts-node -r dotenv/config --files --project tsconfig.json ./src/exchange/exchanges/coinbase/test.ts",
    "push": "git push origin main",
    "pull": "git pull origin main",
    "fullInit": "export GAINIUM_KUCOIN=@gainium/kucoin-api && (npm uninstall $GAINIUM_KUCOIN || true) && npm cache clean --force && npm install $GAINIUM_KUCOIN@git+https://github.com/Gainium/kucoin-api.git && unset GAINIUM_KUCOIN",
    "prepare": "husky"
  },
  "dependencies": {
    "@gainium/kucoin-api": "github:Gainium/kucoin-api",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-express": "^11.1.3",
    "axios": "^1.10.0",
    "binance-api-node": "^0.12.9",
    "bitget-api": "^2.3.5",
    "bybit-api": "^4.1.13",
    "coinbase-advanced-node": "^4.1.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.6.0",
    "isomorphic-unfetch": "^4.0.2",
    "okx-api": "^2.0.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@nestjs/cli": "^11.0.7",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.0.4",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
